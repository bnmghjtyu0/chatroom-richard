    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.2",
    "express": "~4.16.1",
    "http-errors": "^1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.2",
    "pug": "2.0.0-beta11",
    "socket.io": "^2.3.0",
    "use-socketio": "^2.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.23",
    "@types/socket.io": "^2.1.9",
    "debug": "^2.6.9",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
